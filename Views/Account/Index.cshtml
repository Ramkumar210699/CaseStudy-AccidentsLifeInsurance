@model CaseStudy.Models.UsersRegistrationDetail

@{
    ViewBag.Title = "Index";
}




<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
<body>


    <img src=" data:imag0/png;base64,"
         style="width:35vw">



    <div class="well" style="background-color:darkslategrey; color: white;"><b>Our Latest Policies</b></div>

    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTeRbjgs1NkqgAx-M_zfTCz3NzSBVYODvWUhA&usqp=CAU" alt="Life Insurance">
                <div class="carousel-caption">
                    <h3 style="color:black">Life Insurance</h3>
                    <p>Life is Precious</p>
                </div>
            </div>

            <div class="item">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQjdo4HOsk6I2BrbaBQHeJdPpmpsy2xntEk_g&usqp=CAU" alt="Accidents&Vehical Insurance">
                <div class="carousel-caption">
                    <h3 style="color:black">Accidents & Vehical Insurance</h3>

                </div>
            </div>

            <div class="item">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR5g7sS1TE5FttHX4oG-nYA50moFIBKmRHYIQ&usqp=CAU" alt="Product Liability Insurance">
                <div class="carousel-caption">
                    <h3 style="color:black">Liability</h3>

                </div>
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</body>
