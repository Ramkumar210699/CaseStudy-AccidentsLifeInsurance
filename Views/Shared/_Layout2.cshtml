
@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")



    <style>
        .main {
            background-image: url("https://media.istockphoto.com/photos/round-podium-or-pedestal-with-green-backgound-picture-id1299157256?b=1&k=20&m=1299157256&s=170667a&w=0&h=K1_5faLgJFynzHgBRyHrPKJCL24k9Oy_2-NFadPVjiw=");
            background-size: cover;
            height: contain;
        }
    </style>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>



</head>
<body class="main">
    <div class="navbar navbar-inverse navbar-fixed-top">
        @*<div class="container">
                <div class="navbar-header">
                    <h1 id="font-face">Abc Ltd Insurance Company</h1>
                </div>
            </div>*@
        <div class="container">
            <div class="navbar-header">
                <img src=" data:image/png;base64,"
                     style="width:25vh">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    @*<span class="icon-bar"></span>
                    <span class="icon-bar"></span>*@
                </button>
                @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                @*@section login{
            @Html.ActionLink("Login", "Login", "Account")
        }*@
            </div>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav-row">
                    @*<li><a href="/Account/">Home</a></li>
                    <li><a href="/Account/About/">About Us</a></li>*@
                    <li><a href="/Account/">Logout</a></li>
                </ul>
                <div>
                </div>
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div class="row">&nbsp;&nbsp;</div>
 
        <div class="row" style="padding:16vh;">
            <div class="col-md-3">
                <p><a class="btn btn-default" href="~/Admin/PolicyDetails/Index">List Of Policy Details &raquo;</a></p>
                <p><a class="btn btn-default" href="~/Admin/ListofPolicyHolders/Index">List of Policy Holders Details &raquo;</a></p>
                <p><a class="btn btn-default" href="~/Admin/ManagerList/Index">Managers List &raquo;</a></p>
                <p><a class="btn btn-default" href="~/Admin/PremiumPaidList/Index">Premium Paid List &raquo;</a></p>
            </div>
            <div class="col-md-8">  @RenderBody() </div>
        </div>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
